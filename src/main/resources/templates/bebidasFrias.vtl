<h2><font color="white">Bebidas Frias</font></h2>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Precio</th>
    </tr>
  </thead>
  <tbody>
    #foreach($bebidaFria in $bebidasFrias)
      <tr>
        <td><a href="/addCantidad/$bebidaFria.getId()">$bebidaFria.getNombre()</a></td>
        <td>$bebidaFria.getPrecio()</td>
      </tr>
    #end
  </tbody>
</table>
