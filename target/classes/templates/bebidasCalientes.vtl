<h2><font color="white">Bebidas Calientes</font>s</h2>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Precio</th>
    </tr>
  </thead>
  <tbody>
    #foreach($bebidaCaliente in $bebidasCalientes)
      <tr>
        <td><a href="/addCantidad/$bebidaCaliente.getId()">$bebidaCaliente.getNombre()</a></td>
        <td>$bebidaCaliente.getPrecio()</td>
      </tr>
    #end
  </tbody>
</table>
