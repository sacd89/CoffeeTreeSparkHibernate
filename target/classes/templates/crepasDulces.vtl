<h2><font color="white">Crepas Dulces</font></h2>
<table class="table table-hover table-inverse">
  <thead  class="thead-default">
    <tr>
      <th>Nombre</th>
      <th>Precio</th>
    </tr>
  </thead>
  <tbody>
    #foreach($crepaDulce in $crepasDulces)
      <tr class="bg-success">
        <td><a href="/addCantidad/$crepaDulce.getId()">$crepaDulce.getNombre()</a></td>
        <td>$crepaDulce.getPrecio()</td>
      </tr>
    #end
  </tbody>
</table>
